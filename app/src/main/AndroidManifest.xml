<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imgp">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic11"
        android:label="AniFact"
        android:supportsRtl="true"
        android:theme="@style/Theme.Imgp">
        <activity android:name=".AnimalInformation"></activity>
        <activity android:name=".SearchedAnimal" />
        <activity android:name=".cam" />
        <activity
            android:name=".keyword_act"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.Imgp.NoActionBar" />
        <activity
            android:name=".lay1"
            android:label="@string/title_activity_lay1"
            android:theme="@style/Theme.Imgp.NoActionBar" />
        <activity android:name=".options">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>